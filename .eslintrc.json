{
  "ignorePatterns": ["dist", "node_modules", "webpack.*", "config/paths.js"],
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "class-methods-use-this": 0,
    "import/prefer-default-export": 0
  }
}
